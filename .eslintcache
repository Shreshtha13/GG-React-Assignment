' exact=\"true\" component={List} />\n          <Route path={`/:id`} component={DisplaySelectedMovie} />\n        </Switch>\n      </div>\n    </Router>\n  )\n}\n\n\nexport default App;\n","/home/shreshtha13/workSpace/GG/react-assignment/src/components/MoviesList.js",[],"/home/shreshtha13/workSpace/GG/react-assignment/src/components/SelectedMovie.js",["35","36"],"import React, { useState, useEffect } from 'react'\nimport axios from 'axios'\nimport { BrowserRouter as Router, Link, useParams } from 'react-router-dom'\nimport DisplayCard from './DisplayCard'\n\n\nconst SelectedMovie = ({ movie, setMovie }) => {\n\n    const [movieDetails, setMovieDetails] = useState({})\n    let { movieID } = useParams()\n    console.log(movieID);\n    useEffect(() => {\n        const fetchDetails = async () => {\n            const response = await axios.get(`https://www.omdbapi.com/?apikey=7142622a&i=${movie}`)\n            if (response.data.Title !== undefined) {\n                setMovieDetails(response.data)\n            }\n\n        }\n\n        fetchDetails()\n    }, [])\n\n\n    const clearMovieDetail = () => {\n        setMovieDetails({})\n        setMovie({})\n\n    }\n\n    const disp = () => {\n        return (\n            <DisplayCard movieDetails={movieDetails} />\n        )\n    }\n\n    return (\n        <div>\n            <Link to='/' className=\"go-to-home\" >\n                <div onClick={clearMovieDetail}>Home</div>\n            </Link>\n\n            {movieDetails.Title !== undefined && disp()}\n        </div>\n    )\n}\n\nexport default SelectedMovie","/home/shreshtha13/workSpace/GG/react-assignment/src/components/SearchBox.js",[],"/home/shreshtha13/workSpace/GG/react-assignment/src/components/DisplayCard.js",[],{"ruleId":"37","severity":1,"message":"38","line":4,"column":8,"nodeType":"39","messageId":"40","endLine":4,"endColumn":13},{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","replacedBy":"44"},{"ruleId":"37","severity":1,"message":"45","line":4,"column":50